;\r\n\r\n\r\n// Locomotive scroll\r\n// const scroll = new LocomotiveScroll({\r\n//     el: document.querySelector(\"[data-scroll-container]\"),\r\n//     smooth: true\r\n// });\r\n"},"lineCount":null}},"error":null,"hash":"23d64af1e3230f8aec3a68b176a69bec","cacheData":{"env":{}}}